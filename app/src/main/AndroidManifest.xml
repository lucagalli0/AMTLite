<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edqueeneland.amtlite">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.AppCompat.Light">
        <activity android:name="com.edqueeneland.amtlite.StopActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.edqueeneland.amtlite.Insert"
            android:parentActivityName=".StopActivity"/>
        <activity android:name="com.edqueeneland.amtlite.EditActivity"
            android:parentActivityName=".StopActivity"/>/>
        <activity android:name="com.edqueeneland.amtlite.Results"
            android:parentActivityName=".StopActivity"/>/>
    </application>

</manifest>